<div class="column large-4 margin-bottom-two">
  <div class="card relative">
    <%= link_to video, :class=> "ell block video-preview" do %>
    <div class="media-cover flex-embed flex-embed--16by9 margin-bottom-half" style="background-image: url(<%= video.thumbnail_url %>)">
      <div class="card__overlay"></div>
    </div>
    <div class="card-inside">
      <div class="left margin-right-half">
        <%= link_to video.user, :title => video.user.name, :class=> "ell block" do %>
        <img class="left radius-half" src="<%= video.user.image %>" alt="<%= video.user.name %>" style="height:24px;height:24px;">
        <% end %>
      </div>
      <h3 class="ell"><%= video.title %></h3>
    </div>
    <% end %>
  </div>
</div>
